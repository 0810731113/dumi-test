{
  "private": false,
  "name": "@tms/trialos-public-component",
  "version": "1.1.39",
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi buildConf",
    "docs:deploy": "gh-pages -d docs-dist",
    "build": "father-build",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "release": "npm run build && npm publish",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test  --runInBand --detectOpenHandles --unhandled-rejections=strict",
    "test:coverage": "umi-test --coverage",
    "build-lib": "cross-env BUILD_ENV=test webpack --config buildConf/webpack.lib.js --progress"
  },
  "main": "dist/index.umd.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "dependencies": {
    "@reactivex/rxjs": "^6.6.6",
    "@umijs/preset-react": "^1.5.17",
    "alloyfinger": "^0.1.16",
    "axios": "^0.20.0",
    "classnames": "^2.2.6",
    "moment": "^2.24.0",
    "rc-trigger": "^5.2.3",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-draggable": "^4.4.3",
    "rxjs-compat": "^6.6.6",
    "umi": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/plugin-syntax-jsx": "^7.12.13",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-react": "^7.13.13",
    "@jest/types": "^27.0.2",
    "@testing-library/jest-dom": "^5.13.0",
    "@testing-library/react": "^11.2.7",
    "@types/expect-puppeteer": "^4.4.5",
    "@types/jest-environment-puppeteer": "^4.4.1",
    "@types/puppeteer": "^5.4.3",
    "@umijs/test": "^3.0.5",
    "babel-jest": "^27.0.2",
    "babel-types": "^6.26.0",
    "cross-env": "^5.2.0",
    "dumi": "^1.0.10",
    "expect-puppeteer": "^5.0.4",
    "father-build": "^1.17.2",
    "gh-pages": "^3.0.0",
    "jest": "^27.0.4",
    "jest-config": "^27.0.4",
    "jest-puppeteer": "^5.0.4",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "puppeteer": "^10.0.0",
    "puppeteer-core": "^10.0.0",
    "react-test-renderer": "^17.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "ts-node": "^10.0.0",
    "webpack-cli": "^4.3.0",
    "yorkie": "^2.0.0"
  },
  "peerDependencies": {
    "@umijs/preset-react": ">=1.5.17",
    "antd": ">=3.23.6",
    "axios": ">=0.20.0",
    "classnames": ">=2.2.6",
    "dumi": ">=1.0.0",
    "moment": ">=2.24.0",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "umi": ">=3.0.0"
  }
}
